{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name": "php-app",
      "image": "php:7.0-apache",
      "environment": [
        {
          "name": "Container",
          "value": "PHP"
        }
      ],
      "essential": true,
      "memory": 128,
      "mountPoints": [
        {
          "sourceVolume": "php-app",
          "containerPath": "/var/www/html",
          "readOnly": true
        }
      ]
    }
  ]
}
